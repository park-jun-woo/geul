    "parallax": {
      "description": "시차 양자화 (4비트, arcsec)",
      "bits": 4,
      "note": "거리 = 1/parallax(pc)",
      "ranges": [
        {"code": 0, "label": "Unknown"},
        {"code": 1, "range": [null, 0.001], "label": ">1000pc"},
        {"code": 2, "range": [0.001, 0.01], "label": "100-1000pc"},
