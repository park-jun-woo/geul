    "redshift": {
      "description": "적색편이 양자화 (5비트, z값)",
      "bits": 5,
      "note": "로그 스케일, 0~10+ 범위",
      "ranges": [
        {"code": 0, "label": "Unknown"},
        {"code": 1, "range": [null, -0.001], "label": "Blueshift"},
        {"code": 2, "range": [-0.001, 0.001], "label": "Near Zero"},
        {"code": 3, "range": [0.001, 0.01], "label": "Very Low (0.001-0.01)"},
        {"code": 4, "range": [0.01, 0.05], "label": "Low (0.01-0.05)"},
        {"code": 5, "range": [0.05, 0.1], "label": "Moderate Low"},
        {"code": 6, "range": [0.1, 0.2], "label": "Moderate"},
        {"code": 7, "range": [0.2, 0.5], "label": "Moderate High"},
        {"code": 8, "range": [0.5, 1.0], "label": "High"},
        {"code": 9, "range": [1.0, 2.0], "label": "Very High"},
        {"code": 10, "range": [2.0, 3.0], "label": "Ultra High"},
        {"code": 11, "range": [3.0, 5.0], "label": "Extreme"},
        {"code": 12, "range": [5.0, null], "label": "Early Universe (>5)"}
