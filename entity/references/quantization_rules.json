{
  "version": "v0.2",
  "description": "속성 양자화 규칙 정의 - 48비트 전체 활용 버전",
  "rules": {
    "coordinate": {
      "description": "위경도 좌표 양자화",
      "lat_bits": 4,
      "lon_bits": 4,
      "total_bits": 8,
      "lat_zones": [
        {"code": 0, "range": [-90, -60], "label": "Antarctic"},
        {"code": 1, "range": [-60, -40], "label": "SubAntarctic"},
        {"code": 2, "range": [-40, -20], "label": "SouthTemperate"},
        {"code": 3, "range": [-20, 0], "label": "SouthTropic"},
        {"code": 4, "range": [0, 20], "label": "NorthTropic"},
        {"code": 5, "range": [20, 40], "label": "NorthTemperate"},
        {"code": 6, "range": [40, 60], "label": "SubArctic"},
        {"code": 7, "range": [60, 90], "label": "Arctic"}
      ],
      "lon_zones": 16
    },
    "time_era": {
      "description": "시대 양자화 (4비트)",
      "bits": 4,
      "eras": [
        {"code": 0, "label": "Unknown"},
        {"code": 1, "range": [null, -3000], "label": "Prehistoric"},
        {"code": 2, "range": [-3000, 500], "label": "Ancient"},
        {"code": 3, "range": [500, 1000], "label": "Classical"},
        {"code": 4, "range": [1000, 1500], "label": "Medieval"},
        {"code": 5, "range": [1500, 1800], "label": "EarlyModern"},
        {"code": 6, "range": [1800, 1900], "label": "Modern19C"},
        {"code": 7, "range": [1900, 1950], "label": "Modern20CEarly"},
        {"code": 8, "range": [1950, 1980], "label": "Contemporary"},
        {"code": 9, "range": [1980, 2000], "label": "Late20C"},
        {"code": 10, "range": [2000, 2010], "label": "Early21C"},
        {"code": 11, "range": [2010, 2020], "label": "2010s"},
        {"code": 12, "range": [2020, null], "label": "Current"},
        {"code": 13, "label": "Reserved"},
        {"code": 14, "label": "Reserved"},
        {"code": 15, "label": "Reserved"}
      ]
    },
    "time_decade": {
      "description": "10년 단위 (4비트, 시대 종속)",
      "bits": 4,
      "note": "시대 내에서 0-15로 10년 단위 구분"
    },
    "population": {
      "description": "인구수 로그 스케일 (4비트)",
      "bits": 4,
      "ranges": [
        {"code": 0, "range": [0, 100], "label": "<100"},
        {"code": 1, "range": [100, 500], "label": "100-500"},
        {"code": 2, "range": [500, 1000], "label": "500-1K"},
        {"code": 3, "range": [1000, 5000], "label": "1K-5K"},
        {"code": 4, "range": [5000, 10000], "label": "5K-10K"},
        {"code": 5, "range": [10000, 50000], "label": "10K-50K"},
        {"code": 6, "range": [50000, 100000], "label": "50K-100K"},
        {"code": 7, "range": [100000, 500000], "label": "100K-500K"},
        {"code": 8, "range": [500000, 1000000], "label": "500K-1M"},
        {"code": 9, "range": [1000000, 5000000], "label": "1M-5M"},
        {"code": 10, "range": [5000000, 10000000], "label": "5M-10M"},
        {"code": 11, "range": [10000000, null], "label": ">10M"},
        {"code": 12, "label": "Unknown"},
        {"code": 13, "label": "Reserved"},
        {"code": 14, "label": "Reserved"},
        {"code": 15, "label": "Reserved"}
      ]
    },
    "magnitude": {
      "description": "천체 등급 양자화 (4비트)",
      "bits": 4,
      "ranges": [
        {"code": 0, "range": [null, -5], "label": "Very Bright"},
        {"code": 1, "range": [-5, 0], "label": "Bright"},
        {"code": 2, "range": [0, 2], "label": "Visible Naked"},
        {"code": 3, "range": [2, 4], "label": "Dim Naked"},
        {"code": 4, "range": [4, 6], "label": "Limit Naked"},
        {"code": 5, "range": [6, 8], "label": "Binocular"},
        {"code": 6, "range": [8, 10], "label": "Small Telescope"},
        {"code": 7, "range": [10, 12], "label": "Medium Telescope"},
        {"code": 8, "range": [12, 15], "label": "Large Telescope"},
        {"code": 9, "range": [15, 20], "label": "Professional"},
        {"code": 10, "range": [20, null], "label": "Very Faint"}
      ]
    },
    "duration": {
      "description": "상영시간 양자화 (4비트)",
      "bits": 4,
      "ranges": [
        {"code": 0, "range": [0, 30], "label": "Short"},
        {"code": 1, "range": [30, 60], "label": "Medium Short"},
        {"code": 2, "range": [60, 90], "label": "Standard Short"},
        {"code": 3, "range": [90, 105], "label": "Standard"},
        {"code": 4, "range": [105, 120], "label": "Standard Long"},
        {"code": 5, "range": [120, 150], "label": "Long"},
        {"code": 6, "range": [150, 180], "label": "Very Long"},
        {"code": 7, "range": [180, null], "label": "Epic"}
      ]
    },
    "notability": {
      "description": "저명도 (sitelinks 기반, 3비트)",
      "bits": 3,
      "ranges": [
        {"code": 0, "range": [0, 1], "label": "Unknown"},
        {"code": 1, "range": [1, 10], "label": "Minor"},
        {"code": 2, "range": [10, 50], "label": "Notable"},
        {"code": 3, "range": [50, 100], "label": "Well-known"},
        {"code": 4, "range": [100, 200], "label": "Famous"},
        {"code": 5, "range": [200, 500], "label": "Very Famous"},
        {"code": 6, "range": [500, 1000], "label": "Highly Famous"},
        {"code": 7, "range": [1000, null], "label": "World Famous"}
      ]
    },
    "language": {
      "description": "사용 언어 (6비트, P1412 기반)",
      "bits": 6,
      "note": "상위 64개 주요 언어, 빈도 기반 코드북 생성",
      "ranges": [
        {"code": 0, "label": "Unknown"},
        {"code": 1, "label": "English"},
        {"code": 2, "label": "Mandarin"},
        {"code": 3, "label": "Spanish"},
        {"code": 4, "label": "French"},
        {"code": 5, "label": "German"},
        {"code": 6, "label": "Russian"},
        {"code": 7, "label": "Japanese"},
        {"code": 8, "label": "Korean"},
        {"code": 9, "label": "Arabic"},
        {"code": 10, "label": "Portuguese"},
        {"code": 11, "label": "Italian"},
