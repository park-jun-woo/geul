    "radial_velocity": {
      "description": "시선속도 양자화 (5비트, km/s)",
      "bits": 5,
      "ranges": [
        {"code": 0, "label": "Unknown"},
        {"code": 1, "range": [null, -500], "label": "Extreme Approach (<-500)"},
        {"code": 2, "range": [-500, -200], "label": "Fast Approach"},
        {"code": 3, "range": [-200, -100], "label": "Moderate Approach"},
        {"code": 4, "range": [-100, -50], "label": "Slow Approach"},
        {"code": 5, "range": [-50, -20], "label": "Approach"},
        {"code": 6, "range": [-20, -10], "label": "Slight Approach"},
        {"code": 7, "range": [-10, -5], "label": "Minimal Approach"},
        {"code": 8, "range": [-5, -1], "label": "Near Approach"},
        {"code": 9, "range": [-1, 1], "label": "Stationary"},
        {"code": 10, "range": [1, 5], "label": "Near Recede"},
        {"code": 11, "range": [5, 10], "label": "Minimal Recede"},
        {"code": 12, "range": [10, 20], "label": "Slight Recede"},
        {"code": 13, "range": [20, 50], "label": "Recede"},
