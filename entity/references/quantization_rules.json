      "bits": 4,
      "ranges": [
        {"code": 0, "label": "Unknown"},
        {"code": 1, "label": "Arts"},
        {"code": 2, "label": "Science"},
        {"code": 3, "label": "Technology"},
        {"code": 4, "label": "Business"},
        {"code": 5, "label": "Politics"},
        {"code": 6, "label": "Sports"},
        {"code": 7, "label": "Religion"},
        {"code": 8, "label": "Military"},
        {"code": 9, "label": "Law"},
        {"code": 10, "label": "Medicine"},
        {"code": 11, "label": "Education"},
        {"code": 12, "label": "Media"},
        {"code": 13, "label": "Social"},
        {"code": 14, "label": "Other"},
        {"code": 15, "label": "Reserved"}
      ]
